(robin (0 . 1) (small (0 . 1) list (0 . 1) concurrency (0 . 1) crude-io (0 . 1) random (0 . 1))
  (bind output-loop
    (fun (self n)
      (if (equal? n 0)
        (literal done)
        (call random range (list 1 6) value
          (call crude-output write value response
            (self self (subtract n 1))))))
    (output-loop output-loop 20)))
